\ProvidesPackage{constellations}[2024/12/18 A package for drawing constellations with TikZ]
\RequirePackage{tikz}

% Define a command to draw stars
\newcommand{\starAt}[2]{
    \node[circle, fill=black, inner sep=0pt, minimum size=#2] at (#1) {};
}

% Define a command to connect stars
\newcommand{\connectStars}[2]{
    \draw (#1) -- (#2);
}

% Define a command to add constellations
\newcommand{\constellation}[1]{
    \begin{tikzpicture}
        #1
    \end{tikzpicture}
}

